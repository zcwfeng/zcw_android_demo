<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">
    <Button
        android:id="@+id/java_sample_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JavaJNISample" />

    <Button
        android:id="@+id/fmod_demo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ff0000"
        android:text="Fmod 调音师实战" />

    <Button
        android:id="@+id/sample_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!" />

    <Button
        android:id="@+id/mmap_demo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="mmap读写demo" />

    <Button
        android:id="@+id/bspatch_demo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bspatch demo" />

    <TextView
        android:text="更新前~~~~~~~~~~~~~更新后"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>